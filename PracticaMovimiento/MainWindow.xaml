<Window x:Class="PracticaMovimiento.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaMovimiento"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Canvas Visibility="Visible" Name="miCanvas" KeyDown="miCanvas_KeyDown" KeyUp="miCanvas_KeyUp" Focusable="True">
            <Image Name="imgVaquero" Source="q.jpg" Width="90" Height="90" Canvas.Left="343" Canvas.Top="336"></Image>
            <Image Name="carro" Source="carro.jpg" Width="130" Height="80" Canvas.Left="808" Canvas.Top="239"></Image>
            <TextBlock x:Name="lblInterseccionX" Canvas.Left="10" TextWrapping="Wrap" Text="No hay inteseccion en X" Canvas.Top="10"/>
            <TextBlock x:Name="lblInterseccionY" Canvas.Left="10" TextWrapping="Wrap" Text="No hay inteseccion en Y" Canvas.Top="31"/>
            <TextBlock x:Name="lblColision" Canvas.Left="10" TextWrapping="Wrap" Text="No hay colision" Canvas.Top="52"/>
        </Canvas>
        <Canvas Visibility="Collapsed" Name="canvasGameOver">
            <TextBlock Text="Game Over" Canvas.Left="201" Canvas.Top="152" FontSize="72" Foreground="#FFE20909"></TextBlock>
        </Canvas>
    </Canvas>

</Window>
